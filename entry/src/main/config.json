    "package": "com.huawei.zhanjiang",
    "name": ".MyApplication",
    "js": [
      {
        "pages": [
          "pages/index/index"
        ],
        "name": "default",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": true
        }
      }
    ],
    "reqPermissions": [
      {
        "reason": "111",
        "usedScene": {
          "ability": [
            ".MainAbility"
          ],
          "when": "always"
        },
        "name": "ohos.permission.INTERNET"
      }
    ]
